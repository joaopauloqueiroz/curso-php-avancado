<IfModule mod_deflate.c>
  #Aqui utilizando pela extenção do arquivo

  #  <FilesMatch "\.(php|js|css)$">
  #      SetOutputFilter DEFLATE
  #  </FilesMatch>

  #utilizando pelos MIMETypes

   AddOutputFilterByType DEFLATE text/html text/plain text/xml 
   AddOutputFilterByType DEFLATE application/xml application/xhtml+xml 
   AddOutputFilterByType DEFLATE text/javascript text/css 
   AddOutputFilterByType DEFLATE application/javascript application/pdf
</IfModule>